  object qValMom: TZReadOnlyQuery
    Connection = dmPrincipal.zconn
    SQL.Strings = (
      'SELECT'
      '  t1.nome as tipo,'
      '  t2.descricao as descricao,'
      '  CAST(((t2.tempo*60)+t2.tolerancia) as integer) as tempo,'
      '  t2.valor'
      'FROM apartamentos_tipos t1'
      'LEFT JOIN valores_momentos t2 ON t2.id_tipo=t1.id'
      'WHERE t1.nome=:nome_tipo'
      'ORDER BY tempo')
    Params = <
      item
        DataType = ftUnknown
        Name = 'nome_tipo'
        ParamType = ptUnknown
      end>
    Left = 368
    Top = 24
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'nome_tipo'
        ParamType = ptUnknown
      end>
  end
